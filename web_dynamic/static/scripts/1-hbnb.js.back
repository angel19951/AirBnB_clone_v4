$.ready(function () {
  let checkedAmenities = {};
  let checkboxes = ('DIV.amenities input');

  checkboxes.change(function () {
    checkedAmenities = checkboxes
      .filter(":checked")
      .map(box => box.attr('data-name'));

    $('DIV.amenities h4').text(($.map(checkedAmenities, function(value, key) {
      return value})).join(', '));
  });
});
